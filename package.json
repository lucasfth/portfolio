{
  "name": "portfolio",
  "homepage": "https://lucashanson.dk",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@heroui/navbar": "^2.2.9",
    "@heroui/react": "^2.7.9",
    "@shadcn/ui": "^0.0.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.6.1",
    "react-scripts": "5.0.1",
    "react-syntax-highlighter": "^15.6.1",
    "remark-gfm": "^4.0.1",
    "web-vitals": "^5.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && npm run copy-images",
    "normalize-extensions": "find build/images -type f -name '*.JPG' -exec bash -c 'echo Converting \\\"$1\\\" to lowercase; mv \"$1\" \"${1%.JPG}.jpg\"' _ {} \\; && echo 'Done normalizing extensions'",
    "normalize-markdown": "find build/content -type f -name '*.md' -exec sed -i 's/\\.JPG/\\.jpg/g' {} \\;",
    "predeploy": "npm run build && npm run normalize-extensions && npm run normalize-markdown",
    "deploy": "gh-pages -d build",
    "copy-images": "mkdir -p build/images && cp -r public/images/* build/images/",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "file-loader": "^6.2.0",
    "gh-pages": "^6.3.0",
    "raw-loader": "^4.0.2"
  }
}
